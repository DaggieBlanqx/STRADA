{
  "name": "strada",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --open",
    "build": "ng build --prod",
    "test": "ng test",
    "test:ci": "ng test --single-run true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:docs": "compodoc -p src/tsconfig.app.json -d ./docs -n \"STRADA documentation\"",
    "serve:docs": "compodoc -d ./docs -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test:ci"
    }
  },
  "private": true,
