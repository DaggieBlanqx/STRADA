{
  "name": "strada",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --open",
    "build": "ng build --prod",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/strada/stats.json",
    "serve": "firebase serve -p 4200",
    "deploy": "firebase deploy --only hosting",
    "test": "ng test",
    "test:ci": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:docs": "compodoc -p src/tsconfig.app.json -d ./docs -n \"STRADA documentation\" --disableCoverage -a images",
    "serve:docs": "compodoc -d ./docs -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test:ci"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.4",
    "@angular/common": "7.0.4",
    "@angular/compiler": "7.0.4",
    "@angular/core": "7.0.4",
    "@angular/forms": "7.0.4",
    "@angular/http": "7.0.4",
    "@angular/platform-browser": "7.0.4",
    "@angular/platform-browser-dynamic": "7.0.4",
    "@angular/router": "7.0.4",
    "@angular/material": "7.0.4",
    "@angular/cdk": "7.0.4",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@ngrx/store": "6.1.2",
    "@turf/area": "6.0.1",
    "@turf/helpers": "6.1.4",
    "@turf/invariant": "6.1.2",
    "@turf/bearing": "6.0.1",
    "@turf/distance": "6.0.1",
    "@turf/transform-translate": "5.1.5",
    "@turf/centroid": "6.0.2",
    "@turf/center": "6.0.1",
    "ng-pick-datetime": "7.0.0",
    "mout": "1.1.0",
    "everpolate": "0.0.3",
    "chart.js": "2.7.3",
    "chartjs-plugin-annotation": "0.5.7",
    "web-animations-js": "2.3.1",
    "hammerjs": "2.0.8",
    "animejs": "2.2.0",
    "qs": "6.5.2",
    "core-js": "2.5.7",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "7.0.4",
    "@angular-devkit/build-angular": "0.10.6",
    "@angular/cli": "7.0.6",
    "@angular/language-service": "7.0.4",
    "@types/jasmine": "3.3.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.12.9",
    "@types/animejs": "2.0.1",
    "@types/googlemaps": "3.30.16",
    "@types/qs": "6.5.1",
    "@compodoc/compodoc": "1.1.6",
    "codelyzer": "4.5.0",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.1.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "protractor": "5.4.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-angular": "1.1.2",
    "rxjs-tslint": "0.1.5",
    "typescript": "3.1.6",
    "husky": "1.1.4",
    "webpack-bundle-analyzer": "3.0.3"
  }
