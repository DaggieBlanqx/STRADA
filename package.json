{
  "name": "strada",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --open",
    "build": "ng build --prod",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/strada/stats.json",
    "serve": "firebase serve -p 4200",
    "deploy": "firebase deploy --only hosting",
    "test": "ng test",
    "test:ci": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:docs": "compodoc -p src/tsconfig.app.json -d ./docs -n \"STRADA documentation\" --disableCoverage",
    "serve:docs": "compodoc -d ./docs -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test:ci"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.1",
    "@angular/common": "6.1.1",
    "@angular/compiler": "6.1.1",
    "@angular/core": "6.1.1",
    "@angular/forms": "6.1.1",
    "@angular/http": "6.1.1",
    "@angular/platform-browser": "6.1.1",
    "@angular/platform-browser-dynamic": "6.1.1",
    "@angular/router": "6.1.1",
    "@angular/material": "6.4.3",
    "@angular/cdk": "6.4.3",
    "@angular/flex-layout": "6.0.0-beta.17",
    "@ngrx/store": "6.1.0",
    "@turf/area": "6.0.1",
    "@turf/helpers": "6.1.4",
    "@turf/invariant": "6.1.2",
    "@turf/bearing": "6.0.1",
    "@turf/distance": "6.0.1",
    "@turf/transform-translate": "5.1.5",
    "@turf/centroid": "6.0.2",
    "@turf/center": "6.0.1",
    "ng-pick-datetime": "6.0.15",
    "mout": "1.1.0",
    "everpolate": "0.0.3",
    "chart.js": "2.7.2",
    "chartjs-plugin-annotation": "0.5.7",
    "web-animations-js": "2.3.1",
    "hammerjs": "2.0.8",
    "animejs": "2.2.0",
    "qs": "6.5.2",
    "core-js": "2.5.7",
    "rxjs": "6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "6.1.1",
    "@angular-devkit/build-angular": "0.7.3",
    "@angular/cli": "6.1.3",
    "@angular/language-service": "6.1.1",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.5.7",
    "@types/animejs": "2.0.0",
    "@types/googlemaps": "3.30.11",
    "@types/qs": "6.5.1",
    "@compodoc/compodoc": "1.1.3",
    "codelyzer": "4.4.3",
    "jasmine-core": "3.2.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.2.0",
    "protractor": "5.4.0",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "tslint-angular": "1.1.2",
    "rxjs-tslint": "0.1.5",
    "typescript": "2.9.2",
    "husky": "1.0.0-rc.13",
